{
  "name": "chrass",
  "version": "0.3",
  "description": "Chrome Developer Tools Extension for Sass",
  "manifest_version": 2,
  "minimum_chrome_version": "18.0",
  "devtools_page": "devtools.html",
  "icons": { "16": "images/sass16.png",
             "48": "images/sass48.png",
             "128": "images/sass128.png" },
  "background" : {
    "scripts": [
      "background.js"]
  },
  "permissions" :  [
    "tabs",
    "http://*/*",
    "https://*/*",
    "file://*"
  ]
}
